@font-face {
  font-family: 'icomoon';
  src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'), url('icomoon.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"],
[class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-close:before {
  content: "\e900";
}

.icon-silence:before {
  content: "\e901";
}

.icon-setting:before {
  content: "\e902";
}

.icon-sound:before {
  content: "\e903";
}


/*字体图标 end*/

.game-container {
  box-sizing: border-box;
  position: absolute;
  padding: 10px;
  width: 100%;
  height: 100%;
  background: #F8F1E9;
}

.game-container .action-panel {
  box-sizing: border-box;
  width: 100%;
  height: 120px;
}

.game-container .action-panel .left {
  float: left;
  background: #EDC818;
  border-radius: 4px;
}

.game-container .action-panel .left .label {
  margin-top: 10px;
  display: block;
  text-align: center;
  color: #fff;
  line-height: 20px;
}

.game-container .action-panel .left .score {
  display: block;
  text-align: center;
  color: #fff;
  line-height: 80px;
  font-size: 50px;
}

.game-container .action-panel .right {
  box-sizing: border-box;
  float: left;
  margin-left: 10px;
  padding: 10px;
  background: #B9AD9D;
  border-radius: 4px;
}

.btn {
  background: #FF9A3E;
  color: #fff;
}

.game-container .action-panel .right .icons {
  padding-top: 10px;
}

.game-container .action-panel .right .icon {
  display: inline-block;
  margin: 0 5px;
  width: 45px;
  height: 45px;
  border-radius: 50%;
  background: #999999;
  text-align: center;
  line-height: 45px;
  color: #fff;
  font-size: 22px;
  vertical-align: middle;
}

.game-container .action-panel .right .icon:before {
  margin-top: 2px;
}

.game-container .border {
  position: absolute;
  bottom: 20px;
  border: 2px solid #B9AD9F;
  border-radius: 3px;
}

.game-container .sqr-wrap {
  position: absolute;
  width: 100%;
  background: #BAADA4;
}

.game-container .cell {
  position: absolute;
  float: left;
  display: block;
  background: #B9AD9F;
  border: 2px solid #B9AD9F;
  box-sizing: border-box;
}

.game-container .cell .placeholder {
  position: relative;
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 8px;
  background: #CBBFB1;
}

.game-container .cell .true-cell {
  position: absolute;
  z-index: 1;
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 8px;
}

.number {
  display: block;
  position: absolute;
  top: 50%;
  /*left: 50%;*/
  margin-top: -15px;
  /*margin-left: -15px;*/
  width: 100%;
  height: 30px;
  font-size: 18px;
  line-height: 30px;
  text-align: center;
  color: #000;
}

