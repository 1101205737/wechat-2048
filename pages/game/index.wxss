.popout-wrap .mask {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 99;
  width: 100%;
  height: 100%;
  background: #000000;
  opacity: 0.4;
}

.popout-wrap .pop-content {
  box-sizing: border-box;
  position: absolute;
  top: 50%;
  left: 50%;
  z-index: 991;
  transform: translate(-50%, -50%);
  padding: 20px;
  width: 280px;
  height: 320px;
  background: #fff;
  border-radius: 2px;
}

.popout-wrap .pop-content .icon-close {
  position: absolute;
  top: -10px;
  right: -10px;
  width: 30px;
  height: 30px;
  line-height: 30px;
  color: #999;
  font-weight: bold;
  text-align: center;
  border-radius: 50%;
  background: #fff;
}

.popout-wrap .pop-content button {
  margin-top: 20px;
  background: #FF9B3E;
  color: #fff;
}
